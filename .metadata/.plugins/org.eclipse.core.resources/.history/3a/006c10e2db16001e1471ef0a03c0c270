package day11;

public class User {
	private String name;
	private String id;
	private String pw;
	
	// 기본생성자, 모든필드 매개변수로 받는 생성자
	
	public User(String name, String id, String pw) {
		this.name  = name;
		this.id  = id;
		this.pw  = pw;
}
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return name;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getId() {
		return id;
	}
	public void setPw(String pw) {
		this.pw = pw;
	}
	public String getPw() {
		return pw;
	}
	// 메서드 (get/set)
	public void print() {
		System.out.println(name+"\t"+id+"\t"+pw);
	}
	
}
	
	// 메서드 print


