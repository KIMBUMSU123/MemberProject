package day10;

public class CalculatorMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 객체생성 후 각 메서드 호출하여 값 출력
		Calculator  c = new Calculator();
		
		c.num1=10;
		c.num2=5;
		
		c.sum();
		c.sub(10, 2);
		System.out.println(c.mul());
		System.out.println(c.div(10, 5));
	}

}
